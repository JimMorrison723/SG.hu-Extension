{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "useBuiltIns": true,
        "debug": false
      }
    ]
  ],
  "plugins": [
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ],
  "ignore": [
    "node_modules"
  ]
}
